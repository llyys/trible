{
  "name": "tsapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node tools/build",
    "rebuild": "npm run clean && node tools/build-all",
    "start": "tsc && node build/server/server.js",
    "tsc": "tsc",
    "dev": "webpack && tsc-watch --onSuccess \"node build/server/server.js\"",
    "clean": "rimraf build",
    "webpack": "node tools\tasks\build",
    "stats": "webpack --env production --profile --json > stats.json",
    "tcm": "tcm -p src/**/*.scss",
    "db-up": "node_modules/db-migrate/bin/db-migrate up --config src/config/database.json -v --migrations-dir ./migrate/migrations -e prod",
    "test": "jest",
    "server:dev": "tsc && "
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "author": "",
  "license": "ISC",
